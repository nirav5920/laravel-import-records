    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodebyRay",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-medialibrary": "^10.0|^11.0"
